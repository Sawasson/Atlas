@model IEnumerable<Jawabkom_Generator3.Core.RawMonthlyClicks>

<h1 style="text-align:center">Jawabkom - Raw Monthly Clicks</h1>
<br />

<div class="container">

    <table class="table table-bordered table-hover">
        <thead style="background-color:powderblue">
            <tr>
                <th>Created Date </th>
                <th>Country Code </th>
                <th>Clicks</th>
                <th>GDN Clicks</th>
                <th>Total Clicks</th>
            </tr>
        </thead>
        @foreach (var d in Model)
        {
            <tr>
                <td>@d.created_date.ToString("yyyy/MM/dd")</td>
                <td>@d.country_code</td>
                <td>@d.Clicks</td>
                <td>@d.GDNClicks</td>
                <td>@d.Total_Clicks</td>

            </tr>
        }
    </table>
</div>

@section Scripts
{
    <script>
        $('.table').dataTable({
        });
    </script>
}